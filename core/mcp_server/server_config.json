{
    "math": {
        "command": "python",
        "args": [
            "/mnt/e/project/langgraph_mcp_agent/core/mcp_server/tools/math_tools.py"
        ],
        "transport": "stdio"
    },

    "amap-maps-streamableHTTP": {
      "url": "https://mcp.amap.com/mcp?key=e4de13e7805699e85d06ce90e5456c01",
      "transport": "streamable_http"
    },

    "arxiv-mcp-server": {
        "command": "uv",
        "args": [
            "tool",
            "run",
            "arxiv-mcp-server",
            "--storage-path",
            "/mnt/e/arxiv_paper"
        ],
        "transport": "stdio"
    },
    "time-mcp": {
        "command": "npx",
        "args": [
            "-y",
            "time-mcp"
        ],
        "transport": "stdio"
    },
    "LocalResourceServer": {
        "command": "python",
        "args": [
            "/mnt/e/project/langgraph_mcp_agent/core/mcp_server/resources/resource_server.py"
        ],
        "transport": "stdio"
    },
    "firecrawl-mcp": {
        "command": "npx",
        "args": [
            "-y",
            "firecrawl-mcp"
        ],
        "env": {
            "FIRECRAWL_API_KEY": "your_api_key_here"
        },
        "transport": "stdio"
    },
    "DataServer":{
        "command": "python",
        "args": [
            "/mnt/e/project/langgraph_mcp_agent/core/mcp_server/resources/test_resource.py"
