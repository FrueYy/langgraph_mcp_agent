        "transport": "stdio"
    },
    "prompt-mcp": {
        "command": "python",
        "args": [
            "/mnt/e/project/langgraph_mcp_agent/core/mcp_server/prompts/prompt.py"
        ],
        "transport": "stdio"
    }
